{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Student list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Lastname</th>
                <th>Firstname</th>
                <th>Email</th>
                <th>Startdate</th>
                <th>Enddate</th>
                <th>Homephone</th>
                <th>Bussinessphone</th>
                <th>Cellphone</th>
                <th>Faxnumber</th>
                <th>Picture</th>
                <th>Address</th>
                <th>City</th>
                <th>State</th>
                <th>Zip</th>
                <th>Country</th>
                <th>Credittrans</th>
                <th>Refered</th>
                <th>Webpage</th>
                <th>Notes</th>
                <th>Attachment</th>
                <th>Location</th>
                <th>Status</th>
                <th>Comments</th>
                <th>Emergencycontactname</th>
                <th>Emergencycontactphone1</th>
                <th>Emergencycontactphone2</th>
                <th>Emergencycontactphone3</th>
                <th>Cv</th>
                <th>Cvs</th>
                <th>Middleinitial</th>
                <th>Paymentinfo</th>
                <th>Paymentplanamount</th>
                <th>Eo</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('student_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.lastName }}</td>
                <td>{{ entity.firstName }}</td>
                <td>{{ entity.email }}</td>
                <td>{% if entity.startDate %}{{ entity.startDate|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.endDate %}{{ entity.endDate|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.homePhone }}</td>
                <td>{{ entity.bussinessPhone }}</td>
                <td>{{ entity.cellPhone }}</td>
                <td>{{ entity.faxNumber }}</td>
                <td>{{ entity.picture }}</td>
                <td>{{ entity.address }}</td>
                <td>{{ entity.city }}</td>
                <td>{{ entity.state }}</td>
                <td>{{ entity.zip }}</td>
                <td>{{ entity.country }}</td>
                <td>{{ entity.creditTrans }}</td>
                <td>{{ entity.refered }}</td>
                <td>{{ entity.webPage }}</td>
                <td>{{ entity.notes }}</td>
                <td>{{ entity.attachment }}</td>
                <td>{{ entity.location }}</td>
                <td>{{ entity.status }}</td>
                <td>{{ entity.comments }}</td>
                <td>{{ entity.emergencyContactName }}</td>
                <td>{{ entity.emergencyContactPhone1 }}</td>
                <td>{{ entity.emergencyContactPhone2 }}</td>
                <td>{{ entity.emergencyContactPhone3 }}</td>
                <td>{{ entity.cv }}</td>
                <td>{{ entity.cvs }}</td>
                <td>{{ entity.middleInitial }}</td>
                <td>{{ entity.paymentInfo }}</td>
                <td>{{ entity.paymentPlanAmount }}</td>
                <td>{{ entity.eo }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('student_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('student_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('student_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
